"custom/pomodoro": {
  "format": "{}",
  "exec": "$HOME/.local/bin/omarchy-pomodoro-status",
  "return-type": "json",
  "interval": 1,
  "signal": 10,
  "on-click": "$HOME/.local/bin/omarchy-pomodoro toggle",
  "on-click-right": "$HOME/.local/bin/omarchy-pomodoro stop",
  "on-click-middle": "$HOME/.local/bin/omarchy-pomodoro skip"
}
